<% if @product.alcoholic? %>
  <%= text_field_tag :alcohol_shipping_category, ALCOHOLIC_SHIPPING_CATEGORY, disabled: true, class: 'form-control' %>
<% else %>
  <%= f.collection_select(:shipping_category_id, @shipping_categories, :id, :name, { include_blank: Spree.t('match_choices.none') }, { class: 'select2' }) %>
<% end %>
