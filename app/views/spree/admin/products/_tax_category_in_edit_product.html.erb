<% if @product.alcoholic? %>
  <%= text_field_tag :alcohol_tax_category, ALCOHOLIC_TAX_CATEGORY, disabled: true, class: 'form-control' %>
<% else %>
  <%= f.collection_select(:tax_category_id, @tax_categories, :id, :name, { include_blank: Spree.t('match_choices.none') }, { class: 'select2' }) %>
<% end %>
